<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonTPCore">
        <attr name="buttonText" format="string" />
        <attr name="buttonStatus" format="enum">
            <enum name="loading" value="0" />
            <enum name="active" value="1" />
            <enum name="disable" value="2" />
        </attr>
        <attr name="buttonColor" format="color" />
        <attr name="buttonTextColor" format="color" />
    </declare-styleable>

</resources>